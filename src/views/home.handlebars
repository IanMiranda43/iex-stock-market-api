    <h1 class="companyDataTitle">Stock market companies</h1>

    <div class="row">
        <div class="col mb-3">
            <form id="getCompanyData">
                <label for="selectCompany">Select company</label>
                <select id="selectCompany">
                    <option value="appl">Apple</option>
                    <option value="twtr">Twitter</option>
                    <option value="fb">Facebook</option>
                </select>
            </form>
        </div>
    </div>
    
    <div class="companyData">

        <div class="row">
            <div class="col-md-4 mb-4">
                <div class="companyTitle">
                    <h4 class="companyName">{{{ title }}}</h4>
                    <h2 class="companyQuoteValue">{{{ quoteData.latestPrice }}} <strong class="{{{ quoteData.quoteChangeClassView }}}">{{{ quoteData.quoteChange }}}</strong></h2>
                </div>

                <table class="companyQuoteData">
                    <tbody>
                        <tr>
                            <td>Previous close</td>
                            <td>{{{ quoteData.previousClose }}}</td>
                        </tr>
                        <tr>
                            <td>Previous volume</td>
                            <td>{{{ quoteData.previousVolume }}}</td>
                        </tr>
                        <tr>
                            <td>Day's range</td>
                            <td>{{{ quoteData.low }}} - {{{ quoteData.high }}}</td><!-- low - high (both can be null) -->
                        </tr>
                        <tr>
                            <td>Year's change</td>
                            <td>{{{ quoteData.week52Low }}} - {{{ quoteData.week52High }}}</td><!-- week52Low - week52High -->
                        </tr>
                    </tbody>
                </table>
            </div>

            <div class="col-md-8 mb-4">
                <h4 class="companyInfo">Company information</h4>

                <table class="companyDataTable">
                    <tbody>
                        <tr>
                            <td>Industry</td>
                            <td>{{{ companyData.industry }}}</td>
                        </tr>
                        <tr>
                            <td>Web site</td>
                            <td>
                                <a href="{{{ companyData.website }}}">{{{ companyData.website }}}</a>
                            </td>
                        </tr>
                        <tr>
                            <td>Employees</td>
                            <td>{{{ companyData.employees }}}</td>
                        </tr>
                        <tr>
                            <td>Description</td>
                            <td>{{{ companyData.description }}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        
    </div>
